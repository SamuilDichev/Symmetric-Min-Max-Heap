<ivysettings>
  <resolvers>
    <chain name="default">
      <url name="eysys">
        <ivy pattern="http://releases.eysys.com/[module]/ivy-[revision].xml" />
        <artifact pattern="http://releases.eysys.com/[module]/[artifact]-[revision].[ext]" />
      </url>
      <ibiblio name="public" m2compatible="true" />
      <ibiblio name="unidata-releases" root="https://artifacts.unidata.ucar.edu/content/repositories/unidata-releases/" m2compatible="true" />

    </chain>
  </resolvers>
  <modules>
    <!--<module organisation="edu.ucar" resolver="unidata-releases" />-->
  </modules>
</ivysettings>
